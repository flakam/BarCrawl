@model List<Bar>
       
<h2>Pick your starting point</h2>



<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Location
            </th>

            <th>
                Price
            </th>

            <th>
                Rating
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    <a href=@item.Url> @Html.DisplayFor(modelItem => item.Name)</a>
                </td>
                <td>
                    <iframe width="600" height="100" frameborder="1" style="border:1"
                            src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBdac79PQX5UeDPlBxeExEgtE0dYxQmG8s&q=@item.Location" allowfullscreen>
                    </iframe>

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rating)
                </td>
                <td>
                    @Html.ActionLink("Start", "Stops", "Home",
                       new
                            {
                                id = item.BarId,
                                name = item.Name,
                                location = item.Location,
                                longitude = item.Longitude,
                                latitude = item.Latitude,
                                price = item.Price,
                                rating = item.Rating,
                                url = item.Url,
                                num = ViewBag.num
                            }, null)
                </td>

            </tr>
        }



    </tbody>
</table>

